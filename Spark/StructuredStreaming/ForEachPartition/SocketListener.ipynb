    "import socket\n",
    "\n",
    "socket = socket.socket()\n",
    "socket.bind((\"localhost\", 6666))\n",
    "socket.listen(1)\n",
    "print(\"Waitting until connected\")\n",
    "\n",
    "while True:\n",
    "    client, addr= socket.accept()\n",
    "    msg = client.recv(1024)\n",
    "\n",
    "    print(msg)"
